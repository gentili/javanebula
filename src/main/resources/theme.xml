<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE themes PUBLIC "-//www.matthiasmann.de//TWL-Theme//EN" "http://hg.l33tlabs.org/twl/raw-file/tip/src/de/matthiasmann/twl/theme/theme.dtd">
<themes>
    <images file="widgets.png">
        <area name="box" xywh="1,1,42,34" nocenter="true" splitx="L13,R13" splity="T5,B5" border="0,4"/>
        <area name="thinbox" xywh="2,2,40,32" nocenter="true" splitx="L6,R6" splity="T6,B6" border="0,4"/>
        <area name="box bg" xywh="52,6,34,2" border="10" repeatY="true"/>
        <composed name="panel-blue">
            <alias ref="box bg" tint="#066600"/>
            <alias ref="box" tint="#00A400"/>
        </composed>
        <composed name="panel-red">
            <alias ref="box bg" tint="#F22424"/>
            <alias ref="box" tint="#EFA813"/>
        </composed>
        <select name="button.background">
            <alias ref="panel-blue" if="pressed|selected" inset="2,2,-2,-2"/>
            <alias ref="panel-red" if="hover"/>
            <alias ref="panel-blue"/>
        </select>
        <select name="listboxbindings.background">
            <alias ref="none" if="empty"/>
            <alias ref="box bg" if="selected" tint="#AF6716"/>
            <alias ref="box bg" if="hover" tint="#2B2B2B"/>
        
            <alias ref="none"/>
        </select>
        <select name="hscrollbar.button" sizeOverwriteH="26">
            <alias ref="panel-red" if="pressed"/>
            <alias ref="panel-red" if="hover"/>
            <alias ref="panel-blue"/>
        </select>
        <select name="vscrollbar.button" sizeOverwriteV="26" sizeOverwriteH="34">
            <alias ref="panel-red" if="pressed"/>
            <alias ref="panel-red" if="hover"/>
            <alias ref="panel-blue"/>
        </select>
        <grid name="checkbox.background" weightsX="1,1" weightsY="1,1">
            <area xywh="1,1,12,12"/>
            <area xywh="31,1,12,12"/>
            <area xywh="1,23,12,12"/>
            <area xywh="31,23,12,12"/>
        </grid>
        <area name="checkbox.overlay" xywh="127,1,30,31" if="selected" center="true" inset="-12,0,0,-7"/>
        <alias name="radiobutton.background" ref="checkbox.background"/>
        <area name="radiobutton.overlay" xywh="3,36,17,17" if="selected"/>
    
        <alias name="editfield.background" ref="thinbox" tint="#10FF00"/>
    
        <alias name="cosmwindow.background" ref="thinbox" tint="#088300"/>
        <area name="white" xywh="1,36,1,1"/>
        <alias name="black" ref="white" tint="#000000"/>
    
        <alias name="editfield.cursor" ref="white" tint="#0000FF"/>
        <alias name="editfield.selection" ref="white" tint="#80000087"/>
    
    
        <area name="hscrollbar.thumb.overlay" xywh="158,21,5,16" center="true"/>
    
    
        <area name="vscrollbar.thumb.overlay" xywh="158,15,16,5" center="true"/>
        <alias name="listbox.display.background" ref="white" if="selected" tint="#00B011"/>
        <area name="combobox.button.overlay" xywh="158,1,16,13" center="true"/>
        <grid name="dotted box" weightsX="0,1,0" weightsY="0,1,0" inset="6">
            <area xywh="3,36,8,9"/>
            <area xywh="11,36,2,8" repeatX="true"/>
            <area xywh="13,36,7,9"/>
            <area xywh="3,45,8,2" repeatY="true"/>
            <area xywh="9,45,2,2"/>
            <area xywh="13,45,7,2" repeatY="true"/>
            <area xywh="3,45,8,8"/>
            <area xywh="11,45,2,8" repeatX="true"/>
            <area xywh="13,45,7,8"/>
        </grid>
        <select name="focus-rectangle">
            <alias ref="dotted box" if="keyboardFocus+pressed" inset="2,2,-2,-2"/>
            <alias ref="dotted box" if="keyboardFocus"/>
            <alias ref="none"/>
        </select>
    </images>
    <fontDef name="normal" filename="futurenormal.fnt" color="#FFFFFF" default="true">
        <fontParam if="error" color="red"/>
        <fontParam if="warning" color="orange"/>
        <fontParam if="disabled" color="gray"/>
        <fontParam if="textSelection" color="blue"/>
    </fontDef>
    <fontDef name="tiny" filename="skinnytiny.fnt" color="#FFFFFF">
        <fontParam if="error" color="red"/>
        <fontParam if="warning" color="orange"/>
        <fontParam if="disabled" color="gray"/>
        <fontParam if="textSelection" color="blue"/>
    </fontDef>
    <fontDef name="dim" filename="futurenormal.fnt" color="#64FFFFFF">
        <fontParam if="error" color="red"/>
        <fontParam if="warning" color="orange"/>
        <fontParam if="disabled" color="gray"/>
        <fontParam if="textSelection" color="blue"/>
    </fontDef>
    <fontDef name="large" filename="futurelarge.fnt" color="white"/>
    <fontDef name="button.font" filename="futurenormal.fnt" color="#FFFFFF">
        <fontParam if="error" color="red"/>
        <fontParam if="warning" color="orange"/>
        <fontParam if="disabled" color="gray"/>
        <fontParam if="textSelection" color="blue"/>
        <fontParam if="pressed|selected" offsetX="2" offsetY="2"/>
    </fontDef>

    <inputMapDef name="-defaultInputMap">
        <action name="selectAll">ctrl A</action>
        <action name="selectAll">cmd A</action>
        <action name="cut">ctrl X</action>
        <action name="cut">cmd X</action>
        <action name="copy">ctrl C</action>
        <action name="copy">cmd C</action>
        <action name="paste">ctrl V</action>
        <action name="paste">cmd V</action>
    </inputMapDef>

    <theme name="-defaults">
        <param name="background"><image>none</image></param>
        <param name="overlay"><image>none</image></param>
        <param name="font"><font>normal</font></param>
        <param name="textAlignment"><enum type="alignment">left</enum></param>
        <param name="minWidth"><int>0</int></param>
        <param name="minHeight"><int>0</int></param>
        <param name="maxWidth"><int>0</int></param>
        <param name="maxHeight"><int>0</int></param>
        <param name="inputMap"><inputMap>-defaultInputMap</inputMap></param>
    </theme>
    <theme name="-defaultGaps" ref="-defaults">
        <param name="defaultGap"><dimension>0,0</dimension></param>
        <param name="smallGap"><dimension>0,0</dimension></param>
        <param name="mediumGap"><dimension>0,0</dimension></param>
        <param name="largeGap"><dimension>0,0</dimension></param>
        <param name="namedGaps">
            <map>
            </map>
        </param>
    
    
    </theme>
    <theme name="blank" allowWildcard="false"/>


    <theme name="label" ref="-defaults" allowWildcard="true">
        <param name="background"><image>none</image></param>
    </theme>


    <theme name="readoutwidget" allowWildcard="true" ref="-defaults">
        <param name="background"><image>none</image></param>
        <param name="font"><font>tiny</font></param>
        <param name="linespeed"><int>50</int></param>
    
    </theme>
    <theme name="button" ref="-defaults">
        <param name="background"><image>button.background</image></param>
        <param name="border"><border>6,10,7,10</border></param>
        <param name="font"><font>button.font</font></param>
        <param name="overlay"><image>focus-rectangle</image></param>
    </theme>
    <theme name="panel" ref="-defaults">
        <param name="background"><image>panel-blue</image></param>
    </theme>
    <theme name="togglebutton" ref="button"/>
    <theme name="checkbox" ref="-defaults">
        <param name="background"><image>checkbox.background</image></param>
        <param name="overlay"><image>checkbox.overlay</image></param>
    </theme>
    <theme name="radiobutton" ref="-defaults">
        <param name="background"><image>radiobutton.background</image></param>
        <param name="overlay"><image>radiobutton.overlay</image></param>
    </theme>
    <theme name="editfield" ref="-defaults">
        <param name="border"><border>10,0</border></param>
        <theme name="renderer" ref="-defaults"/>
        <param name=""><image>editfield.*</image></param>
        <param name="passwordChar"><int>42</int></param>
    </theme>
    <theme name="cosmwindow" ref="-defaults">
        <param name="border"><border>10,0</border></param>
        <param name=""><image>cosmwindow.*</image></param>
    </theme>
    <theme name="cosmtextarea" ref="-defaults">
        <param name="font"><font>tiny</font></param>
        <param name="textAlignment"><enum type="alignment">BOTTOMLEFT</enum></param>
    </theme>

    <theme name="hscrollbar" ref="-defaults">
        <theme name="leftbutton" ref="-defaults">
            <param name="background"><image>hscrollbar.button</image></param>
        </theme>
        <theme name="rightbutton" ref="-defaults">
            <param name="background"><image>hscrollbar.button</image></param>
        </theme>
        <theme name="thumb" ref="-defaults">
            <param name="background"><image>hscrollbar.button</image></param>
            <param name="overlay"><image>hscrollbar.thumb.overlay</image></param>
        </theme>
        <param name="scaleThumb"><bool>false</bool></param>
        <param name="background"><image>box</image></param>
    </theme>
    <theme name="vscrollbar" ref="-defaults">
        <theme name="upbutton" ref="-defaults">
            <param name="background"><image>vscrollbar.button</image></param>
        </theme>
        <theme name="downbutton" ref="-defaults">
            <param name="background"><image>vscrollbar.button</image></param>
        </theme>
        <theme name="thumb" ref="-defaults">
            <param name="background"><image>vscrollbar.button</image></param>
            <param name="overlay"><image>vscrollbar.thumb.overlay</image></param>
        </theme>
        <param name="scaleThumb"><bool>false</bool></param>
        <param name="background"><image>box</image></param>
    </theme>
    <theme name="scrollpane" ref="-defaults">
        <theme name="hscrollbar" ref="hscrollbar"/>
        <theme name="vscrollbar" ref="vscrollbar"/>
        <param name="hasDragButton"><bool>true</bool></param>
        <param name="scrollbarsAlwaysVisible"><bool>true</bool></param>
        <param name="autoScrollArea"><int>0</int></param>
        <param name="autoScrollSpeed"><int>0</int></param>
        <theme name="dragButton" ref="button"/>
    </theme>
    <theme name="scrollinglogpane" ref="-defaults">
        <theme name="textarea" ref="-defaults">
            <param name="fonts">
                <map>
                    <param name="default"><font>normal</font></param>
                </map>
            </param>
            <param name="images">
                <map>
                </map>
            </param>
        </theme>
        <theme name="hscrollbar" ref="-defaults">
            <param name="scaleThumb"><bool>false</bool></param>
            <theme name="leftbutton" ref="-defaults"/>
            <theme name="rightbutton" ref="-defaults"/>
            <theme name="thumb" ref="-defaults"/>
        
        
        </theme>
        <theme name="vscrollbar" ref="-defaults">
            <param name="scaleThumb"><bool>false</bool></param>
            <theme name="upbutton" ref="-defaults"/>
            <theme name="downbutton" ref="-defaults"/>
            <theme name="thumb" ref="-defaults"/>
        
        
        
        </theme>
        <param name="hasDragButton"><bool>false</bool></param>
        <param name="scrollbarsAlwaysVisible"><bool>false</bool></param>
        <param name="autoScrollArea"><int>0</int></param>
        <param name="autoScrollSpeed"><int>0</int></param>
    
    </theme>
    <theme name="textarea" ref="-defaults">
        <param name="fonts">
            <map>
                <param name="default"><font>normal</font></param>
            </map>
        </param>
        <param name="images">
            <map>
            </map>
        </param>
    </theme>
    <theme name="listbox" ref="-defaults">
        <param name="background"><image>panel-blue</image></param>
        <theme name="display" ref="label">
            <param name="background"><image>listbox.display.background</image></param>
        </theme>
        <param name="border"><border>10,5</border></param>
        <theme name="vscrollbar" ref="vscrollbar"/>
        <param name="maxHeight"><int>150</int></param>
    </theme>
    <theme name="listboxbindings" ref="-defaults">
        <theme name="display" ref="label">
            <param name="background"><image>listboxbindings.background</image></param>
            <param name="font"><font>dim</font></param>
        </theme>
        <param name="border"><border>10,5</border></param>
        <theme name="vscrollbar" ref="vscrollbar">
        </theme>
        <param name="background"><image>none</image></param>
        <param name="fixedCellWidth"><bool>false</bool></param>
        <param name="fixedCellHeight"><bool>true</bool></param>
        <param name="rowMajor"><bool>false</bool></param>
        <param name="minDisplayedRows"><int>15</int></param>
        <param name="cellWidth"><int>90</int></param>
        <param name="cellHeight"><int>15</int></param>
    
    
    
    
    </theme>
    <theme name="combobox" ref="panel">
        <theme name="display" ref="label">
            <param name="border"><border>0,10,0,0</border></param>
        </theme>
        <theme name="button" ref="button">
            <param name="overlay"><image>combobox.button.overlay</image></param>
        </theme>
    </theme>
    <theme name="comboboxPopup" ref="-defaults">
        <theme name="listbox" ref="listbox"/>
    </theme>
    <theme name="testclient" ref="-defaults">
        <theme name="fpscounter" ref="-defaults"/>
    </theme>
    <images file="ditheredgorzo.png">
        <area name="gorzo" xywh="*" center="true"/>
    </images>
    <theme name="splashwidget" ref="-defaults">
        <param name="background"><image>black</image></param>
    </theme>
    <theme name="mainmenuwidget" ref="-defaultGaps">
        <theme name="button" ref="button">
        </theme>
    
        <param name="defaultGap"><dimension>5,5</dimension></param>
    </theme>
    <theme name="tooltipwindow" ref="-defaults">
        <param name="fadeInTime"><int>5</int></param>
    </theme>
    <theme name="settingsdialog" ref="-defaultGaps">
        <param name="background"><image>panel-blue</image></param>
        <theme name="label" ref="-defaults">
        </theme>
        <param name="border"><border>9,10,9,9</border></param>
        <theme name="editfield" ref="editfield">
            <param name="columns"><int>15</int></param>
            <param name="autocompletion-height"><int>0</int></param>
        </theme>
        <theme name="button" ref="button"/>
        <param name="defaultGap"><dimension>5,5</dimension></param>
    
    </theme>
    <theme name="popupwindow" ref="-defaults">
        <param name="background"><image>panel-blue</image></param>
        <theme name="label" ref="-defaults">
            <param name="font"><font>large</font></param>
        </theme>
        <param name="border"><border>10,15</border></param>
    </theme>
    <images file="customWidgets.png">
        <area name="targetbox" xywh="2,2,29,29" nocenter="true" splitx="L3,R3" splity="T3,B3"/>
        <animation name="targetboxpulse" timeSource="pulse">
            <repeat>
                <frame duration="1000" ref="targetbox" tint="#00FA00"/>
                <frame duration="1000" ref="targetbox" tint="#4000FA00"/>
            </repeat>
        
        </animation>
        <animation name="targetboxgrow" timeSource="grow">
            <frame duration="100" ref="targetbox" tint="#00FF00" zoom="0.2"/>
            <frame duration="500" ref="targetbox" tint="#00FF00" zoom="1.0"/>
            <frame duration="0" ref="targetbox" tint="#00FF00" zoom="1.0"/>
        </animation>
        <animation name="targetboxsnap" timeSource="snap">
            <frame duration="200" ref="targetbox" tint="#0000FF00"/>
            <frame duration="500" ref="targetbox" tint="#00FF00" zoom="1.0"/>
            <frame duration="0" ref="targetbox" tint="#00FF00" zoom="1.0"/>
        </animation>
        <select name="targetboxselect">
            <alias ref="targetboxgrow" if="grow"/>
            <alias ref="targetboxsnap" if="snap"/>
            <alias ref="targetboxpulse" if="pulse"/>
            <alias ref="targetbox" tint="#00FF00"/>
        </select>
    </images>
    <theme name="targetwidget" ref="-defaults">
        <theme name="targetbox" ref="-defaults">
            <param name="background"><image>targetboxselect</image></param>
        </theme>
    </theme>


</themes>
